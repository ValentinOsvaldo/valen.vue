{
  "singleQuote": true, 
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "all", 
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "rangeStart": 0,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.{json,css,scss,less,html,js,jsx,ts,tsx,vue}",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}